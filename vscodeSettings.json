{
  "terminal.external.osxExec": "iTerm.app",
  "terminal.explorerKind": "external",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "window.zoomLevel": 1,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.formatOnSaveTimeout": 1000,
  "editor.tabCompletion": "on",
  "mocha-snippets.semicolon": false,
  "explorer.confirmDelete": false,
  "workbench.iconTheme": "vscode-icons",
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontLigatures": true,
  "editor.fontWeight": "500",
  "javascript.autoClosingTags": true,
  "javascript.suggest.autoImports": true,
  "vscode_custom_css.imports": [
    "file:///Users/abhijeet.daspatnaik/Public/synthwave84.css"
  ],
  "explorer.confirmDragAndDrop": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "prettier.singleQuote": true,
  "bracket-pair-colorizer-2.colors": [
    "Gold",
    "Orchid",
    "LightSkyBlue",
    "LightBlue",
    "Pink",
    "Violet",
    "Orange",
    "Yellow",
    "LightGreen",
    "LightIndigo",
    "LightBrown"
  ],
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.forceIterationColorCycle": true,
  "bracket-pair-colorizer-2.colorMode": "Independent",
  "bracket-pair-colorizer-2.activeScopeCSS": [
    "borderStyle : solid",
    "borderWidth : 2px",
    "borderColor : {color}",
    "opacity: 0.5"
  ],
  "editor.matchBrackets": false,
  "bracket-pair-colorizer-2.showVerticalScopeLine": true,
  "bracket-pair-colorizer-2.showHorizontalScopeLine": true,
  "emmet.showSuggestionsAsSnippets": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.localityBonus": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.acceptSuggestionOnEnter": "on",
  "editor.quickSuggestionsDelay": 10,
  "editor.suggestOnTriggerCharacters": true,
  "editor.suggestSelection": "recentlyUsed",
  "editor.wordBasedSuggestions": true,
  "editor.parameterHints.enabled": true,
  "prettier.alignObjectProperties": true,
  "terminal.integrated.enableBell": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "liveServer.settings.port": 3000,
  "breadcrumbs.enabled": true,
  "editor.stablePeek": true,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.fontSize": 15,
  "terminal.integrated.fontSize": 13,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "editor.formatOnSave": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.accessibilitySupport": "on",
  "files.autoSave": "onFocusChange",
  "workbench.startupEditor": "newUntitledFile",
  "docker.commands.build": null,
  "[dockerfile]": {
    "editor.autoIndent": "none",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
